    "schemas": {
      "ProductRequest": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "nullable": true
          },
          "productDetails": { },
          "templateReferenceId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "TemplateRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "templateJson": { }
        },
        "additionalProperties": false
      }
    }
  }
